/*
 * EXAMPLE 8: Specific Channel ID
 *
 * This example demonstrates how to send notifications to a specific channel
 * by ID instead of name.
 *
 * Features enabled:
 * - Notifications sent to 'scidev-slacktest'
 * - Default message templates
 */

plugins {
    id 'nf-slack@0.2.1'
}

slack {
    bot {
        token = System.getenv('SLACK_BOT_TOKEN')
        // You can use a channel ID (e.g. 'C12345678') or a channel name
        channel = 'C12345678'
    }
}
